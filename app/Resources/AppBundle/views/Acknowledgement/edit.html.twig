{% extends 'base.html.twig' %}

{% block content %}
    <div class="col-xs-12">
        <h2 class="h1">
            Edit acknowledgements
        </h2>
    </div>
    <div id="acknowledgements-edit-app">
        <acknowledgements-edit-app
            init-urls="{{ urls }}"
            init-data="{{ acknowledgements }}"
        />
    </div>
{% endblock %}

{% block javascript %}
    <script type="text/javascript" src="{{ asset("/build/main.js") }}"></script>
    <script type="text/javascript" src="{{ asset("/build/acknowledgementsedit.js") }}"></script>
{% endblock %}
