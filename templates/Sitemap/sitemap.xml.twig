<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for part, lastMod in parts %}
        <sitemap>
            <loc>{{ url('sitemap_part', {'part': part}) }}</loc>
            <lastmod>{{ lastMod|date('c') }}</lastmod>
        </sitemap>
    {% endfor %}
</sitemapindex>
