{% extends 'base.html.twig' %}

{% block content %}
    <div class="col-xs-12">
        <h2 class="h1">
            Edit {{ isSubject == "true" ? 'keywords' : 'tags' }}
        </h2>
    </div>
    <div id="keywords-edit-app">
        <keywords-edit-app
            init-urls="{{ urls }}"
            init-data="{{ keywords }}"
            init-persons="{{ persons }}"
            init-is-subject="{{ isSubject }}"
        />
    </div>
{% endblock %}

{% block javascript %}
    {{ vite_entry_script_tags('main') }}
    {{ vite_entry_script_tags('keywordsedit') }}
{% endblock %}
